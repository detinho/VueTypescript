{
    "compilerOptions": {
      "lib": ["dom", "es5", "es2015"],
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "src/",
      "paths": {
        "@/models/*": ["models/*"],
        "@/components/*": ["components/*"]
      },
      "strictNullChecks": true,
      "noImplicitAny": true,
      "alwaysStrict": true,
      "experimentalDecorators": true
    }
}
